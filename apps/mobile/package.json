{
  "name": "@palmera/mobile",
  "version": "1.0.0",
  "private": true,
  "main": "App.js",
  "scripts": {
    "start": "pnpm expo start --clear --port 8081",
    "start:clean": "pkill -f 'expo start' 2>/dev/null || true && pnpm expo start --clear --port 8081",
    "start:dev": "pnpm expo start --dev-client",
    "android": "pnpm expo run:android",
    "ios": "pnpm expo run:ios",
    "web": "pnpm expo start --web --port 8081",
    "build": "echo 'Mobile app build - use expo start for development'",
    "build:android": "pnpm eas build --platform android",
    "build:ios": "pnpm eas build --platform ios",
    "submit": "pnpm eas submit",
    "lint": "pnpm eslint . --ext .ts,.tsx",
    "type-check": "pnpm tsc --noEmit",
    "clean": "rm -rf .expo",
    "doctor": "pnpm expo doctor"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.3.0",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-navigation/bottom-tabs": "^6.5.0",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "@tanstack/react-query": "^4.41.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "date-fns": "^2.30.0",
    "expo": "~51.0.0",
    "expo-camera": "~15.0.16",
    "expo-constants": "~16.0.2",
    "expo-device": "~6.0.2",
    "expo-image-picker": "~15.1.0",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-localization": "~15.0.3",
    "expo-notifications": "~0.28.19",
    "expo-router": "~3.5.24",
    "expo-secure-store": "~13.0.2",
    "expo-status-bar": "~1.12.1",
    "i18next": "^23.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.47.0",
    "react-i18next": "^13.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-maps": "1.14.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-toast-message": "^2.1.0",
    "react-native-vector-icons": "^10.0.0",
    "react-native-web": "~0.19.6",
    "react-native-worklets": "^0.5.1",
    "zod": "^3.22.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-expo": "^7.0.0",
    "metro": "~0.80.8",
    "metro-config": "~0.80.8",
    "metro-resolver": "~0.80.8",
    "typescript": "~5.3.3"
  }
}
