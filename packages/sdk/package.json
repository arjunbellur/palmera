    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@palmera/schemas": "file:../schemas",
    "axios": "^1.5.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "eslint": "^8.0.0",
    "tsup": "^7.0.0",
    "typescript": "^5.2.2"
  }
}
